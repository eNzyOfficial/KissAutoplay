{
  "name": "Kiss Autoplay",
  "version": "0.0.3",
  "manifest_version": 2,
  "description": "Autoplay next video on Kisscartoon and Kissanime.",
  "homepage_url": "https://github.com/eNzyOfficial/kissautoplay",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": true
  },
  "options_page": "src/options_custom/index.html",
  "permissions": [
    "notifications",
    "storage",
    "*://kissanime.ac/Anime/*",
    "*://kisscartoon.ac/Cartoon/*",
    "*://kissasian.ch/Drama/*"
  ],
  "omnibox": {
    "keyword": "kiss"
  },
  "content_scripts": [
    {
      "matches": [
        "*://kissanime.ac/Anime/*",
        "*://kisscartoon.ac/Cartoon/*",
        "*://kissasian.ch/Drama/*"
      ],
      "css": [
        "src/inject/inject.css"
      ]
    },
    {
      "matches": [
        "*://kissanime.ac/Anime/*",
        "*://kisscartoon.ac/Cartoon/*",
        "*://kissasian.ch/Drama/*"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}